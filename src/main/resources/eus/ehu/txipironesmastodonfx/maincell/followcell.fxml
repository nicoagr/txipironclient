<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchor" prefHeight="90.0" prefWidth="795.0" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1">
   <ImageView fx:id="icon" fitHeight="75.0" fitWidth="75.0" layoutX="14.0" layoutY="8.0" onMouseClicked="#pickClick"
              pickOnBounds="true" preserveRatio="true"/>
   <Label fx:id="id" layoutX="111.0" layoutY="14.0" prefHeight="17.0" prefWidth="211.0" text="Id" wrapText="true">
      <font>
         <Font size="8.0"/>
      </font>
   </Label>
   <Label fx:id="username" layoutX="111.0" layoutY="36.0" onMouseClicked="#clickedThings" text="\@username"
          textFill="#0645ad" underline="true">
      <font>
         <Font size="13.0"/>
      </font>
   </Label>
   <Button layoutX="630.0" layoutY="10.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="122.0"
           text="Follow//TODO"/>
</AnchorPane>
